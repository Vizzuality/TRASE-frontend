<% if (multiple) { %>
  <% nodes.forEach(function(node, i) { %>
    <div class="column">
      <% if (i === 0) { %>
        <div class="column-title"><%= node.columnName %></div>
      <% } %>
      <div class="column-content">
        <%= node.name %>
        <a href="./factsheet.html#<%= node.id %>" class="outside-link">
          <svg class="icon icon-outside-link"><use xlink:href="#icon-outside-link"></use></svg>
        </a>
      </div>
    </div>

  <% }) %>
<% } else { %>

  <div class="column">
    <div class="column-title"><%= node.columnName %></div>
    <div class="column-content"><%= node.name %></div>
  </div>

  <div class="column">
    <div class="column-title"><%= node.quantName %></div>
    <div class="column-content"><%= node.quantNice %>&thinsp;<span class="unit"><%= node.quantUnit %></span></div>
  </div>

  <% node.inds.forEach(function(ind) { %>
    <div class="column">
      <div class="column-title"><%= ind.name %></div>
      <div class="column-content"><%= ind.values[0] %></div>
      <!-- <span class="unit"><%= ind.unit %></span> -->
    </div>
  <% }) %>


  <div class="column">
    <div class="column-title">view more</div>
    <div class="column-content">
      <a href="./factsheet.html#<%= node.id %>" class="outside-link">
        <svg class="icon icon-outside-link"><use xlink:href="#icon-outside-link"></use></svg>
      </a>
    </div>
  </div>
<% } %>
