<% groups.forEach(function(group) { %>
  <div class="map-sidebar-group js-map-sidebar-group" data-group-id="<%= group.group.id %>">
    <div class="map-sidebar-group-title js-map-sidebar-group-title">
      <%= group.group.name %>
    </div>
    <ul class="map-sidebar-group-items">
      <% group.dimensions.forEach(function(dimension) { %>
        <li class="map-sidebar-group-item js-map-sidebar-group-item" data-dimension-uid="<%= dimension.uid %>">
          <%= dimension.name %>
          <% if (dimension.unit) { %>
            (<%= dimension.unit %>)
          <% } %>
          <div class="actions">
            <% if (dimension.description) {  %>
              <svg class="icon icon-layer-info js-tooltip" data-tooltip-text="<%= dimension.description %>">
                <use xlink:href="#icon-layer-info"></use>
              </svg>
              <% } %>
            <div class="c-radio-btn" value="<%= dimension.uid %>"></div>
          </div>
        </li>
      <% }) %>
    </ul>
  </div>
<% }) %>
