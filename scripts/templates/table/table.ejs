<table>
  <% if(type === 't_head_places'){ %>
    <thead>
      <tr class="table-row">
        <th class="header-cell">Key sustainability indicators</th>
        <th class="header-cell _text-align-right">Score</th>
        <!-- <th>State Ranking</th> -->
      </tr>
    </thead>
    <tbody>
      <% for(var i = 0; i < data['rows'].length; i++) { %>
        <%  for(var j = 0; j < data['rows'][i]['values'].length; j++){%>
        <tr class="table-row">
          <td class="cell-name">
            <span class="node-name"><%= data['includedColumns'][j]['name'] %></span>
          </td>
          <td class="cell-score _text-align-right">
            <span class="unit" data-unit="<%= i == 0 ? data['includedColumns'][j]['unit'] : null  %>">
              <%= data['rows'][i]['values'][j] %>
            </span>
          </td>
        </tr>
        <% } %>
      <% } %>
    </tbody>
  <% } else if(type === 't_head_actors'){ %>
    <thead>
      <tr class="table-row">
        <% for(var i = 0; i < data['includedColumns'].length; i++) { %>
          <th class="header-cell <% if(i > 0) { %> _text-align-right <% } %>">
            <%= data['includedColumns'][i]['name'] %>
          </th>
        <% } %>
      </tr>
    </thead>
    <tbody>
      <% for(var i = 0; i < data['rows'].length; i++) { %>
      <tr class="table-row">
        <% for(var j = 0; j < data['rows'][i]['values'].length; j++){%>
          <td class="cell-score <% if(j > 0) { %> _text-align-right <% } %>">
            <% if(data['rows'][i]['values'][j] != null){ %>
              <span class="unit" data-unit="<%= i == 0 ? data['includedColumns'][j]['unit'] : null  %>">
                <%= data['rows'][i]['values'][j]['value'] %>
              </span>
            <% } else{ %>
              <span class="unit">N/A</span>
            <% } %>
          </td>
        <% } %>
      </tr>
      <% } %>
    </tbody>
  <% } else {%>
    <tbody>
      <% for(var i = 0; i < data.length; i++) { %>
        <tr class="table-row">
          <td class="cell-name">
            <span class="node-name"><%= i+1 %>.<%= data[i]['name'] %></span>
            <% if (link !== null) {%>
            <a class="node-link" href="<%= link + data[i]['id'] %>">
              <svg class="icon icon-check">
                  <use xlink:href="#icon-outside-link" />
              </svg>
            </a>
            <% } %>
          </td>
          <td class="cell-score _text-align-right">
            <span class="unit" data-unit="<%= i == 0 ? '%' : null  %>">
              <%= data[i]['value'] %>
            </span>
          </td>
        </tr>
      <% } %>
    </tbody>
  <% } %>
</table>
