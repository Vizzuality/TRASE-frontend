<h2><%= title %></h2>
<table>
<% list.forEach(function(node, i) { %>
  <tr>
    <td><%= i + 1 %>.</td>
    <td>
      <%= node.name %>
      <% if (node.link) { %>
        <a href="<%= node.link %>" class="outside-link">
          <svg class="icon icon-outside-link"><use xlink:href="#icon-outside-link"></use></svg>
        </a>
      <% } %>
    </td>
    <td>
      <%= node.formattedValue %>
      <% if (i === 0) { %>
        %
      <% } %>
    </td>
  </tr>
<% }) %>
</table>
