<div class="c-posts js-posts-slider">
<% posts.forEach(function(post) { %>
  <div class="post">
    <figure class="post-image"
      <% if (post['image_url']) { %>
        style="background-image: url('<%= post['image_url'] %>')"
      <% } %>
    ></figure>
    <figcaption>
      <% if (post['title']) { %>
      <%= post['title'] %>
      <% } %>
    </figcaption>
  </div>
<% }) %>
</div>